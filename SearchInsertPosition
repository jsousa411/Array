// traverse array and return index of value = or greater 
// than target
public class Solution {
    public int SearchInsert(int[] nums, int target) {
    
        int i = 0; // index of target value
        for(i = 0; i < nums.Length; i++)
        {
            if(nums[i] >= target)
                return i;
           
        }
        
        return i;
        
    }
}// end insert position
